<?xml version="1.0" encoding="utf-8"?>
<com.dualvectorfoil.eyeslink.mvp.ui.widget.UrlInfoTagLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="blocksDescendants">

    <ImageView
        android:id="@+id/launcher_item_image"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="10dp"
        android:background="#FFF9F9F9"/>

    <TextView
        android:id="@+id/launcher_item_text"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_below="@id/launcher_item_image"
        android:gravity="center_vertical|center_horizontal"
        android:textColor="#000000"
        android:textSize="14dp"/>

</com.dualvectorfoil.eyeslink.mvp.ui.widget.UrlInfoTagLayout>